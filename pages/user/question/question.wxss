.user {
  width: 100%;
  overflow: hidden;
}

.header-container {
  width: 100%;
  height: 200rpx;
  padding: 20rpx;
  align-items: center;
  display: flex;
}

/* 常见问题 */
.question {
  display: flex;
  width: 100%;
  height: 100%;
  align-items: center;
  padding-left: 20px;
  background-color: #626262;
  border-bottom: solid #e4e3e2 1px;
  border-top: solid #e4e3e2 1px;
  color: white;
}